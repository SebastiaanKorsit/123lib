<!DOCTYPE html>
<html>
    <head>
        {{> partial_head }}
        
        <link rel="stylesheet" href="{{ provider.getLocation }}css/stylesheet.css" type="text/css" />
    </head>
    <body>
        <header class="container">
            <div class='grid grid-responsive'>
                <div class="col col-3" style="padding: 0;">
                    {{> partial_logo }}
                </div>

                <div class='col col-3 font-align-center' style="padding: 0; text-align: right;">
                    <a href="//123giftcard.nl/levering" class="font font-color-light font-size-small" style="color: #777;"><i class="icon icon-mail"></i> Activatiecodes <span class="font font-weight-bold font-color-dark">direct</span> geleverd  per e-mail</a>
                </div>
                <div class='col col-3 font-align-center' style="padding: 0; text-align: right;">
                    <a href="//123giftcard.nl/levering" class="font font-color-light font-size-small" style="color: #777;"><i class="icon icon-clock"></i> Binnen 30 seconden geleverd</a>
                </div>
                <div class='col col-3 font-align-center' style="padding: 0; text-align: right;">
                    <a href="//123giftcard.nl/levering" class="font font-color-light font-size-small" style="color: #777;">Veilig betalen met iDeal</a>
                </div>
            </div>
            
        </header>
        
        <nav class="menu container">
            <div class="">
                <div class='grid grid-responsive'>
                    <div class='col col-10 font-align-center'>
                        {{# menu }}
                        <a href="{{ getLocation }}" class="item {{ isActive }}">
                            {{ getName }}
                        </a>
                        {{/ menu }}
                    </div>
                    
                    <div class='col col-2 font-align-center'>
                        
                        <a href='//123giftcard.nl/winkelwagen' class='item {{ cartPageIsActive }}' style='width: 100%;'>
                            <span style='vertical-align: middle;'>Winkelwagen</span>
                            <span class='well well-color-white' style='margin-left: 10px; vertical-align: middle;' id='cart-count'>
                                {{ cart.getTotalCount }}
                                {{^ cart.getTotalCount }}
                                &nbsp;
                                {{/ cart.getTotalCount }}
                            </span>
                        </a>
                    </div>
                </div>
            </div>
        </nav>
        
        <ol class='breadcrumb container' style="margin-bottom: 50px;">
            <li>
                <a href='//123giftcard.nl'>
                    <i class='icon icon-home icon-align-baseline'></i>
                </a>
            </li>
            {{# breadcrumb }}
            <li>
                <a href='{{ getLocation }}'>{{ getName }}</a>
            </li>
            {{/ breadcrumb }}
        </ol>
        
        <!-- start partial entry -->
        <div id="entry">
            {{> partial_entry }}
        </div>
        <!-- end partial entry -->
        <footer class="container" style="margin-top: 70px;">
            <div class="container grid grid-responsive">
                <div class='col col-3'>
                    <h3>Voorwaarden</h3>
                    <ul class='list'>
                        <li>
                            <a href='//123giftcard.nl/voorwaarden'>Algemene voorwaarden</a>
                        </li>
                        <li>
                            <a href='//123giftcard.nl/privacy'>Privacy beleid</a>
                        </li>
                        <li>
                            <a href='//123giftcard.nl/disclaimer'>Disclaimer</a>
                        </li>
                    </ul>
                </div>
                <div class='col col-3'>
                    <h3>Contact & Support</h3>
                    <ul class='list'>
                        <li><a href='//123giftcard.nl/klantenservice'>Klantenservice</a></li>
                        <li><a href='//123giftcard.nl/contact'>Contact</a></li>
                    </ul>
                </div>
                <div class='col col-3'>
                    <h3>Navigatie</h3>
                    <ul class='list'>
                        {{# menu }}
                        <li>
                            <a href="{{ getLocation }}">
                            {{ getName }}
                            </a>
                        </li>
                        {{/ menu }}
                        <li>
                            <a href='sitemap'>Sitemap</a>
                        </li>
                    </ul>
                </div>
                <div class='col col-3'>
                    <h3>Affiliates</h3>
                    <ul class='list' id="affiliates">
                        <li><img src="{{ provider.getLocation }}favicon.ico" alt="itunes kaart icon" width="16" height="16" /><a href='//123ituneskaart.nl'><b>itunes</b>kaart</a></li>
                        <li><img src="{{ provider.getLocation }}favicon.ico" alt="spotify giftcard icon" width="16" height="16" /><a href='//123spotifygiftcard.nl'><b>spotify</b>giftcard</a></li>
                        <li><img src="{{ provider.getLocation }}favicon.ico" alt="google play giftcard icon" width="16" height="16" /><a href='//123googleplaygiftcard.nl'><b>googleplay</b>giftcard</a></li>
                    </ul>
                </div>
            </div>
            <div id='copyright'>
                Â©2014 Korsit - Alle genoemde prijzen zijn inclusief BTW<br />
                The product names used on this website are for identification purposes only.
                All trademarks and registered trademarks are the property of their respective owners.
            </div>
        </footer>
        
        <script src='//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.0/jquery.min.js'></script>
        <script>
            (function (sources) {
                for (var i = 0; i < sources.length; i++) {
                    var head = document.getElementsByTagName('head')[0];
                    var script = document.createElement('script');
                    script.type = 'text/javascript';
                    script.src = sources[i];
                    head.appendChild(script);
                }
            })([
                '{{ provider.getLocation }}js/combined.js'
            ]);
        </script>
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

            
            
            ga('create', 'UA-50002306-1', 'auto', {'allowLinker': true});
            ga('require', 'linker');
            var otherDomains = [];
            {{# otherDomainArray }}
                otherDomains.push('{{ . }}');
            {{/ otherDomainArray }}
            
            ga('linker:autoLink', otherDomains);
            ga('send', 'pageview');
        </script>
    </body>
</html>
